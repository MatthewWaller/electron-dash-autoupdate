{
  "name": "electron-dash-autoupdate",
  "version": "1.0.45",
  "description": "An Electron app with embedded Dash Python app and auto-update functionality",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build-python": "pyinstaller --onefile --distpath dist/python dash_app.py",
    "dev": "electron . --dev",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "electron-builder --publish=always"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "electron-updater": "^6.1.7"
  },
  "author": "Matthew Waller",
  "license": "MIT"
}